#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandergreer/Documents/agur-Network/agur-ui/node_modules/vite-node/node_modules:/Users/alexandergreer/Documents/agur-Network/agur-ui/node_modules:/Users/alexandergreer/Documents/agur-Network/node_modules:/Users/alexandergreer/Documents/node_modules:/Users/alexandergreer/node_modules:/Users/node_modules:/node_modules:/Users/alexandergreer/Documents/agur-Network/agur-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandergreer/Documents/agur-Network/agur-ui/node_modules/vite-node/node_modules:/Users/alexandergreer/Documents/agur-Network/agur-ui/node_modules:/Users/alexandergreer/Documents/agur-Network/node_modules:/Users/alexandergreer/Documents/node_modules:/Users/alexandergreer/node_modules:/Users/node_modules:/node_modules:/Users/alexandergreer/Documents/agur-Network/agur-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../vite-node/vite-node.mjs" "$@"
fi
